{
  "expo": {
    "name": "Happy-Space",
    "slug": "HappySpace",
    "description": "A happy space to get some inspiration, Dad Jokes, see what happened today in history, and Advice from Cats.",
    "version": "1.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.robertbalonek.happy-place",
      "buildNumber": "1.1.0"
    },
    "android": {
      "package": "com.robertbalonek.happyplace",
      "versionCode": 4,
      "permissions": [],
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-font"
    ],
    "scheme": "happyspace"
  }
}
